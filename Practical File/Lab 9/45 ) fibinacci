#include <stdio.h>

int fibonacci(int n) {
    if (n <= 1)
        return n;
    else
        return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n = 25; 
    printf("First %d terms of the Fibonacci sequence:\n\n", n);
    for (int i = 0; i < n; i++) {
        printf("%d ", fibonacci(i));
    }
    printf("\n");
     
     printf("\nWritten by Rajveer Singh\nMEB2-2330082\n");

    return 0;
}
