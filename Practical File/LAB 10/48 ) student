#include<stdio.h>
#include<string.h>
 struct student{
   char name[50];
   int crn;
int urn;
int agg_marks;
 }record[4],p[50];

 int main(){
    int i;
    for (  i = 0; i < 5; i++)
    {
       printf("Enter name of %d student : ",i+1);
       scanf("%s",record[i].name);
      printf("Enter college roll number : ");
       scanf("%d",&record[i].crn);
       printf("Enter universiy roll numuber : ");
       scanf("%d",&record[i].urn);
       printf("Enter aggregate marks : ");
       scanf("%d",&record[i].agg_marks);
    }
    printf("\n\nhere is the output\n\n");
    for ( i = 0; i < 5; i++)
    {
         printf(" name of %d student :  %s",i+1,record[i].name);
        printf(" college roll number : %d\n",record[i].crn);
       printf(" universiy roll numuber : %d\n",record[i].urn);
       printf(" aggregate marks : %d\n",record[i].agg_marks);
    }
    
   return 0;

 }
