#include <stdio.h>

int main()
{
    printf("Rajveer\nMEB2-2330082\n");
     printf("Enter obtained marks\n");
     float a,b,c,d,e,aver,sum,perc,perc1,tm;
     scanf("%f",&a);
     scanf("%f",&b);
     scanf("%f",&c);
     scanf("%f",&d);
     scanf("%f",&e);
     aver=(a+b+c+d+e)/5;
      printf("%f\n",aver);
      scanf("%f",&tm);
      sum=a+b+c+d+e;
      perc1=sum*100;
      perc=perc1/tm;
      printf("%f\n",perc);
     
    printf("Rajveer\nMEB2-2330082");

     return 0;
}  
