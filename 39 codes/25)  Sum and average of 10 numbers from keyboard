#include<stdio.h>
int main()
{
    float sum=0;
    float kb[10];
    printf("Enter 10 numbers : \n");
    
    
    for (int i = 0; i < 10; i++)
    {
       scanf("%f",&kb[i]);
    }
    for (int i = 0; i < 10; i++)
    {
        sum+=kb[i];
    }
    
    printf("Sum of numbers is : %.2f \n",sum);
    printf("Average of these numbers is : %.2f\n",sum/10);
       

   
    
    printf("Written by Rajveer Singh\nMEB2-2330082\n");
}
