#include<stdio.h>
int main ()
{
   
   int a,b,*c,*d;
  printf("Enter first value : ");
  scanf("%d",&a);
  printf("Enter second value : ");
scanf("%d",&b);
c=&a;
d=&b;
  
  printf("Division of these two numbers is : %d\n",*c / *d);
   printf("Their address is : %p\n",*c / *d);
  
  
   printf("\nWritten by Rajveer Singh\nMEB2-2330082\n");

      return 0;
    
  
}
