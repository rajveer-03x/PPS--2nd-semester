}person1,p[50],p1[50];

int main()
{
    printf("Enter name : ");
    gets(person1.name);

    printf("Enter college roll number : ");
    scanf("%d",&person1.crn);

    printf("Enter university roll number : ");
    scanf("%d",&person1.urn);

    printf("Enter branch : ");
    gets(person1.branch);

    printf(" name : %s \n",person1.name);
    
