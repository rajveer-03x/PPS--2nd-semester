#include <stdio.h>


int main() {
    
    int xyz[2][2]={1,2,3,4};
    int abc[2][2]={4,3,2,1};
    int new[2][2];
    for (int  i = 0; i < 2; i++)
    {
      for (int j = 0; j<2 ; j++)
      {
       printf("sum[%d][%d]=%d\n",i+1,j+1,xyz[i][j]+abc[i][j]) ;
      }
      
    }
  

  
     for (int  i = 0; i < 2; i++)
    {
      for (int j = 0; j<2 ; j++)
      {
       new[i][j]=xyz[i][j]+abc[i][j];
       printf("new[%d][%d]=%d\n",i+1,j+1,new[i][j]);
      }
      
    }
   
  
   
   printf("Written by Rajveer Singh\nMEB2-2330082\n");
   return 0;
}
